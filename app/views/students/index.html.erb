
<h1>Listing Student</h1>
<div class = "table">
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Contact</th>
         </tr>
    </thead>
    <tbody>
   
    <% @students.each do |student| %>
    <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.email %></td>
        <td><%= student.contact %></td>

    </tr>
    <% end %>
    
    </tbody>
</table>
</div>