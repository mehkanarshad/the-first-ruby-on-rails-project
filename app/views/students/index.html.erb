<h1>Listing Student</h1>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>First Name</th>
            <th>First Name</th>

        </tr>
    </thead>
    <tbody>
    <% @students.each do |student| %>
    <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.email %></td>
    </tr>
    <% end %>
    </tbody>
</table>